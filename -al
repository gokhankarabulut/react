[32m+[m[32mpublic class BlogApiImpl implements IBlogApi<BlogDto> {[m
[32m+[m
[32m+[m[32m    // INJECTION[m
[32m+[m[32m    private final BlogServicesImpl blogServices;[m
[32m+[m
[32m+[m[32m    // ERROR[m
[32m+[m[32m    private ApiResult apiResult;[m
[32m+[m
[32m+[m[32m    /////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    // ALL CREATE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/speed/10[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @GetMapping("/speed/{data}")[m
[32m+[m[32m    public ResponseEntity<List<BlogDto>> speedData(@PathVariable(name = "data") Long data) {[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.speedData(data));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // ALL DELETE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/delete/all[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @DeleteMapping("/delete/all")[m
[32m+[m[32m    public ResponseEntity<String> allDelete() {[m
[32m+[m[32m        return  ResponseEntity.ok(blogServices.allDelete());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    // CREATE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/create[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @PostMapping("/create")[m
[32m+[m[32m    public ResponseEntity<?> blogServiceCreate(@Valid @RequestBody BlogDto blogDto) {[m
[32m+[m[32m        if(blogDto==null){[m
[32m+[m[32m            throw new ResourceNotFoundException(" Blog Dto null ");[m
[32m+[m[32m        }[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.blogServiceCreate(blogDto));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // LIST[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/list[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @GetMapping("/list")[m
[32m+[m[32m    public ResponseEntity<List<BlogDto>> blogServiceList() {[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.blogServiceList());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // FIND BY ID[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/find[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/find/0[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/find/1[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @GetMapping("/find/{id}")[m
[32m+[m[32m    public ResponseEntity<BlogDto> blogServiceFindById(@PathVariable(name = "id") Long id) {[m
[32m+[m[32m        // id=0 id=null[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.blogServiceFindById(id));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // DELETE[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @DeleteMapping("/delete/{id}")[m
[32m+[m[32m    public ResponseEntity<Map<String, Boolean>> blogServiceDeleteId[m
[32m+[m[32m        (@PathVariable(name = "id") Long id) {[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.blogServiceDeleteId(id));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // UPDATE[m
[32m+[m[32m    @Override[m
[32m+[m[32m    @PutMapping("/update/{id}")[m
[32m+[m[32m    public ResponseEntity<BlogDto> blogServiceUpdateId[m
[32m+[m[32m       (@PathVariable(name = "id") Long id, @Valid @RequestBody BlogDto blogDto) {[m
[32m+[m[32m        return ResponseEntity.ok(blogServices.blogServiceUpdateId(id,blogDto));[m
[32m+[m[32m    }[m
[32m+[m[32m} //end class[m
