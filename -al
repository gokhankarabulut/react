[32m+[m[32mimport axios from "axios";[m
[32m+[m
[32m+[m[32m// Persist  URL[m
[32m+[m[32mconst BLOG_URL = "/blog/api/v1";[m
[32m+[m
[32m+[m[32mclass BlogApi {[m
[32m+[m
[32m+[m[32m    /////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    // ALL CREATE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/speed/10[m
[32m+[m[32m    speedData(data) {[m
[32m+[m[32m        return axios.get(`${BLOG_URL}/speed/10`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // ALL DELETE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/delete/all[m
[32m+[m[32m    allDelete() {[m
[32m+[m[32m        return axios.delete(`${BLOG_URL}/delete/all`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    // CREATE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/create[m
[32m+[m[32m    // {[m
[32m+[m[32m    //     "header":"header data",[m
[32m+[m[32m    //     "content":"content data",[m
[32m+[m[32m    // }[m
[32m+[m[32m    blogServiceCreate(blogDto) {[m
[32m+[m[32m        return axios.post(`${BLOG_URL}/create`, blogDto);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // LIST[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/list[m
[32m+[m[32m    blogServiceList() {[m
[32m+[m[32m        return axios.get(`${BLOG_URL}/list`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // FIND BY ID[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/find/1[m
[32m+[m[32m    blogServiceFindById(id) {[m
[32m+[m[32m        return axios.get(`${BLOG_URL}/find/${id}`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // DELETE[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/delete/1[m
[32m+[m[32m    blogServiceDeleteId(id) {[m
[32m+[m[32m        return axios.delete(`${BLOG_URL}/delete/${id}`);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // UPDATE[m
[32m+[m[32m     // {[m
[32m+[m[32m    //     "header":"header data",[m
[32m+[m[32m    //     "content":"content data",[m
[32m+[m[32m    // }[m
[32m+[m[32m    // http://localhost:4444/blog/api/v1/update/24[m
[32m+[m[32m    blogServiceUpdateId(id, blogDto) {[m
[32m+[m[32m        return axios.put(`${BLOG_URL}/update/${id}`, blogDto);[m
[32m+[m[32m    }[m
[32m+[m[32m} //end class[m
